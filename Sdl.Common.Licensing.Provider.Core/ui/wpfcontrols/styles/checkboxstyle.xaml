<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:themes="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Aero" x:Uid="ResourceDictionary_1">
  <SolidColorBrush x:Key="TextColor" x:Uid="SolidColorBrush_1">#0D1E2D</SolidColorBrush>
  <Style x:Key="Sdl.Checkbox.NoHighlightStyle" x:Uid="Style_1" TargetType="{x:Type CheckBox}">
    <Setter x:Uid="Setter_11" Property="FontSize" Value="11" />
    <Setter x:Uid="Setter_12" Property="FontFamily" Value="Segoe UI" />
    <Setter x:Uid="Setter_13" Property="Foreground" Value="{StaticResource TextColor}" />
  </Style>
  <Style x:Key="Sdl.Checkbox.GenericStyle" x:Uid="Style_2" TargetType="{x:Type CheckBox}" BasedOn="{StaticResource Sdl.Checkbox.NoHighlightStyle}">
    <Style.Triggers>
      <Trigger x:Uid="Trigger_1" Property="ToggleButton.IsChecked" Value="True">
        <Setter x:Uid="Setter_14" Property="FontWeight" Value="Bold" />
      </Trigger>
    </Style.Triggers>
  </Style>
</ResourceDictionary>